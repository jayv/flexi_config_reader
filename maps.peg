grammar Maps
  map     <-  "{" PAIR ("," PAIR)* "}" %make_map
	PAIR    <-  string ":" value %make_pair
  value   <-  list / number / string
  string  <-  '"' [^"]* '"' %make_string
  list    <-  "[" value ("," WS value)* "]" %make_list
  number  <-  [0-9]+ ("." [0-9]*)? WS %make_number
  WS <-  [ \t\r\n]*

