grammar Maps
  map     <-  "{" _ PAIR (COMMA PAIR)* "}" %make_map
  PAIR    <-  string ":" _ value %make_pair
  value   <-  list / number / string
  string  <-  '"' [^"]* '"' _ %make_string
  list    <-  SBO value ("," _ value)* SBC %make_list
  number  <-  [0-9]+ ("." [0-9]*)? _ %make_number
  SBO     <-  "[" _
  SBC     <-  "]" _
  COMMA   <-  "," _
  _       <-  [ \t\r\n]*

